  UniGUIVars, uniGUIMainModule, MainModule;

function DMPerms: TDMPerms;
begin
  Result := TDMPerms(DM.GetModuleInstance(TDMPerms));
end;

initialization
  RegisterModuleClass(TDMPerms);

end.
