  TDMPerms = class(TDataModule)
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  //ESCALES

    can_create

  //RADES


  //FACTURATIONS


  //REGLEMENTS



  //USERS



  //BAREME




  //STATISTIQUES



  //PARAMETRES


